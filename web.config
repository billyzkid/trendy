<?xml version="1.0"?>
<configuration>
	<system.webServer>
		<httpProtocol>
			<customHeaders>
				<!-- disable caching during development -->
				<add name="Cache-Control" value="no-cache" />
				<add name="Pragma" value="no-cache" />
				<add name="Expires" value="-1" />
			</customHeaders>
		</httpProtocol>
	</system.webServer>
</configuration>
